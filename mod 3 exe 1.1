#include <stdio.h>

int even(int a , int b) 
{
int i;
for(i=a;i<=b;i++)
{
if(a%2==0)
{
printf("\n%d",i++);
}
else
{
printf("\n%d",++i);
}
}
}
int odd(int a, int b)
{
int i;
for (i=a;i<=b;i++)
{
if(a%2==0)
{
printf("\n%d",++i);
}
else
{
printf("\n%d",i++);
}
}

}
int main()
{
    int x,y,z,m;
