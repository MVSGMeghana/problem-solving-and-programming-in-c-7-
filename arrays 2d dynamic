#include<stdio.h>
int main()
{
    int a[10][10],i,j,row,col;
    printf("\n Enter row and col value");
    scanf("%d%d",&row,&col);  
    printf("Enter the elements into array");
    for(i=0;i<row;i++)   
    {
        for(j=0;j<col;j++)  
        {
            scanf("%d",&a[i][j]);
        }
    }
    printf("The elements in Matrix is:\n");
    for(i=0;i<row;i++)   
    {
        for(j=0;j<col;j++)  
        {
            printf("%d ",a[i][j]);
        }
    
    printf("\n");
}
}
