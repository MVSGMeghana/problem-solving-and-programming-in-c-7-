#include <stdio.h>

int main()
{
int x,y,*a,*b,temp;
 printf("enter x");
scanf("%d",&x);
 printf("enter y");
scanf("%d",&y);
printf("before swapping x,y are");
printf("\n%d",x);
printf("\n%d",y);
a=&x;
b=&y;
temp=*a;
*a=*b;
*b=temp;
printf("\nafter swapping x,y are");
printf("\n%d",x);
printf("\n%d",y);
    return 0;
}
