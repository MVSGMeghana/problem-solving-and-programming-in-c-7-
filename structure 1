#include<stdio.h>
#include<string.h>
int main()
{
struct car
{
    char name[30];
    char company[30];
    double price;
    int year;
    char model[30];
};
struct car c1,c2,c3;
printf("Enter the car name:");
scanf("%s",&c1.name);
printf("Enter the company name:");
scanf("%s",&c1.company);
printf("Enter the price:");
scanf("%f",&c1.price);
printf("Enter year of manifacturing:");
scanf("%d",&c1.year);
printf("Enter car model");
scanf("%s",&c1.model);
printf("\n%s",c1.name);
printf("\n%s",c1.company);
printf("\n%0.2f",c1.price);
printf("\n%d",c1.year);
printf("\n%s",c1.model);
}
