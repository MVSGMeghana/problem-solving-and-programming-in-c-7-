#include <stdio.h>
int main() {
    int a[100],n,i,large=0;
    printf("enter elements size");
    scanf("%d",&n);
    printf("enter elements");
    for(i=0;i<n;i++)
    {
    scanf("%d",&a[i]);
    }
    for(i=0;i<n;i++)
    {
    if(a[i]>large)
    {
    large=a[i];
    }
    }
    for(i=0;i<n;i++)
    {
    if(a[i]<large)
    {
    large=a[i];
    }
    }
    printf("min is %d",large);
    
    return 0;
}
